// This is your Prisma schema file,
// learn more about it in the docs: https://pris.ly/d/prisma-schema

generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "mysql"
  url      = env("DATABASE_URL")
}

model Players {
  id        Int      @id
  username  String
  message   String   @default("")
  imgURL    String   @default("http://localhost:8000/assets/images/guest.png")
  points    Int
  speed     Float
  boardSize Int
  length    Int
  hours     Int
  minutes   Int
  seconds   Int
  published DateTime
}
